<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Lookbook - Genuine Blanks</title>
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: 'Arial', sans-serif;
            background: #000;
            color: #fff;
            overflow-x: hidden;
        }

        .lookbook-container {
            height: 100vh;
            display: flex;
            position: relative;
        }

        .lookbook-section {
            width: 50%;
            height: 100vh;
            position: relative;
            cursor: pointer;
            overflow: hidden;
            transition: all 0.6s cubic-bezier(0.23, 1, 0.320, 1);
        }

        .lookbook-section:hover {
            transform: scale(1.02);
        }

        .luxury-section {
            background: linear-gradient(135deg, rgba(0,0,0,0.7), rgba(0,0,0,0.4)), 
                        url('data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" width="100" height="100" viewBox="0 0 100 100"><rect width="100" height="100" fill="%23111"/><circle cx="50" cy="50" r="30" fill="%23222"/></svg>');
            background-size: cover;
            background-position: center;
            border-right: 1px solid rgba(255,255,255,0.1);
        }

        .highend-section {
            background: linear-gradient(135deg, rgba(20,20,20,0.7), rgba(40,40,40,0.4)), 
                        url('data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" width="100" height="100" viewBox="0 0 100 100"><rect width="100" height="100" fill="%23333"/><polygon points="50,10 90,90 10,90" fill="%23444"/></svg>');
            background-size: cover;
            background-position: center;
        }

        .section-overlay {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0,0,0,0.3);
            transition: all 0.4s ease;
        }

        .lookbook-section:hover .section-overlay {
            background: rgba(0,0,0,0.1);
        }

        .section-content {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            text-align: center;
            z-index: 10;
            transition: all 0.4s ease;
        }

        .lookbook-section:hover .section-content {
            transform: translate(-50%, -50%) scale(1.05);
        }

        .section-title {
            font-size: 3.5rem;
            font-weight: 300;
            letter-spacing: 0.2em;
            margin-bottom: 1rem;
            text-transform: uppercase;
            text-shadow: 2px 2px 4px rgba(0,0,0,0.5);
        }

        .luxury-section .section-title {
            background: linear-gradient(45deg, #fff, #f0f0f0);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
        }

        .highend-section .section-title {
            background: linear-gradient(45deg, #fff, #ccc);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
        }

        .section-subtitle {
            font-size: 1.2rem;
            font-weight: 300;
            letter-spacing: 0.1em;
            opacity: 0.9;
            margin-bottom: 2rem;
            text-transform: uppercase;
        }

        .section-description {
            font-size: 1rem;
            line-height: 1.6;
            opacity: 0.8;
            max-width: 300px;
            margin: 0 auto 2rem;
            font-weight: 300;
        }

        .explore-btn {
            display: inline-block;
            padding: 15px 40px;
            border: 2px solid rgba(255,255,255,0.3);
            background: rgba(255,255,255,0.05);
            color: #fff;
            text-decoration: none;
            text-transform: uppercase;
            letter-spacing: 0.15em;
            font-weight: 400;
            font-size: 0.9rem;
            transition: all 0.3s ease;
            backdrop-filter: blur(10px);
        }

        .explore-btn:hover {
            background: rgba(255,255,255,0.15);
            border-color: rgba(255,255,255,0.6);
            transform: translateY(-2px);
            box-shadow: 0 10px 30px rgba(0,0,0,0.3);
        }

        .page-header {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            z-index: 20;
            padding: 2rem;
            background: linear-gradient(180deg, rgba(0,0,0,0.8) 0%, transparent 100%);
        }

        .header-content {
            display: flex;
            justify-content: space-between;
            align-items: center;
            max-width: 1400px;
            margin: 0 auto;
        }

        .logo {
            font-size: 1.5rem;
            font-weight: 300;
            letter-spacing: 0.2em;
            text-transform: uppercase;
        }

        .nav-home {
            color: rgba(255,255,255,0.8);
            text-decoration: none;
            font-size: 0.9rem;
            letter-spacing: 0.1em;
            text-transform: uppercase;
            transition: all 0.3s ease;
        }

        .nav-home:hover {
            color: #fff;
        }

        .divider-line {
            position: absolute;
            top: 0;
            left: 50%;
            width: 1px;
            height: 100vh;
            background: linear-gradient(to bottom, 
                transparent 0%, 
                rgba(255,255,255,0.1) 20%, 
                rgba(255,255,255,0.3) 50%, 
                rgba(255,255,255,0.1) 80%, 
                transparent 100%);
            z-index: 15;
        }

        .luxury-accent {
            position: absolute;
            top: 20%;
            right: 10%;
            width: 2px;
            height: 60%;
            background: linear-gradient(to bottom, transparent, rgba(255,255,255,0.3), transparent);
            opacity: 0.6;
        }

        .highend-accent {
            position: absolute;
            top: 20%;
            left: 10%;
            width: 2px;
            height: 60%;
            background: linear-gradient(to bottom, transparent, rgba(255,255,255,0.3), transparent);
            opacity: 0.6;
        }

        @media (max-width: 768px) {
            .lookbook-container {
                flex-direction: column;
            }
            
            .lookbook-section {
                width: 100%;
                height: 50vh;
            }
            
            .luxury-section {
                border-right: none;
                border-bottom: 1px solid rgba(255,255,255,0.1);
            }
            
            .section-title {
                font-size: 2.5rem;
            }
            
            .divider-line {
                top: 50%;
                left: 0;
                width: 100vw;
                height: 1px;
            }
            
            .page-header {
                padding: 1rem;
            }
        }

        .click-indicator {
            position: absolute;
            bottom: 3rem;
            left: 50%;
            transform: translateX(-50%);
            font-size: 0.8rem;
            opacity: 0.6;
            letter-spacing: 0.1em;
            text-transform: uppercase;
            animation: pulse 2s infinite;
        }

        @keyframes pulse {
            0%, 100% { opacity: 0.6; }
            50% { opacity: 1; }
        }

        .luxury-section::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: radial-gradient(circle at 30% 70%, rgba(255,255,255,0.03) 0%, transparent 50%);
            pointer-events: none;
        }

        .highend-section::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: radial-gradient(circle at 70% 30%, rgba(255,255,255,0.03) 0%, transparent 50%);
            pointer-events: none;
        }
    </style>
</head>
<body>
    <div class="page-header">
        <div class="header-content">
            <div class="logo">Genuine Blanks</div>
            <a href="#" class="nav-home">Back to Home</a>
        </div>
    </div>

    <div class="lookbook-container">
        <div class="divider-line"></div>
        
        <div class="lookbook-section luxury-section" onclick="navigateToLuxury()">
            <div class="section-overlay"></div>
            <div class="luxury-accent"></div>
            <div class="section-content">
                <h1 class="section-title">Luxury</h1>
                <p class="section-subtitle">Collection</p>
                <p class="section-description">
                    Discover our premium selection of luxury blanks crafted for the most discerning brands and designers.
                </p>
                <a href="#" class="explore-btn">Explore Luxury</a>
            </div>
            <div class="click-indicator">Click to Enter</div>
        </div>

        <div class="lookbook-section highend-section" onclick="navigateToHighEnd()">
            <div class="section-overlay"></div>
            <div class="highend-accent"></div>
            <div class="section-content">
                <h1 class="section-title">High End</h1>
                <p class="section-subtitle">Collection</p>
                <p class="section-description">
                    Explore our sophisticated high-end blanks designed for elevated fashion and premium applications.
                </p>
                <a href="#" class="explore-btn">Explore High End</a>
            </div>
            <div class="click-indicator">Click to Enter</div>
        </div>
    </div>

    <script>
        function navigateToLuxury() {
            // Add smooth transition effect
            document.querySelector('.luxury-section').style.transform = 'scale(1.1)';
            
            setTimeout(() => {
                // Replace with your actual luxury lookbook URL
                window.location.href = '#luxury-lookbook';
                console.log('Navigating to Luxury Lookbook');
            }, 300);
        }

        function navigateToHighEnd() {
            // Add smooth transition effect
            document.querySelector('.highend-section').style.transform = 'scale(1.1)';
            
            setTimeout(() => {
                // Replace with your actual high-end lookbook URL
                window.location.href = '#highend-lookbook';
                console.log('Navigating to High End Lookbook');
            }, 300);
        }

        // Add subtle parallax effect on mouse move
        document.addEventListener('mousemove', (e) => {
            const mouseX = e.clientX / window.innerWidth;
            const mouseY = e.clientY / window.innerHeight;
            
            document.querySelector('.luxury-accent').style.transform = `translateY(${mouseY * 20 - 10}px)`;
            document.querySelector('.highend-accent').style.transform = `translateY(${mouseY * -20 + 10}px)`;
        });

        // Smooth hover effects
        const sections = document.querySelectorAll('.lookbook-section');
        sections.forEach(section => {
            section.addEventListener('mouseenter', () => {
                section.style.filter = 'brightness(1.1)';
            });
            
            section.addEventListener('mouseleave', () => {
                section.style.filter = 'brightness(1)';
            });
        });
    </script>
</body>
</html>