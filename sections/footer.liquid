{% comment %}theme-check-disable UndefinedObject{% endcomment %}
{{ 'section-footer.css' | asset_url | stylesheet_tag }}
{{ 'component-newsletter.css' | asset_url | stylesheet_tag }}
{{ 'component-list-menu.css' | asset_url | stylesheet_tag }}
{{ 'component-list-payment.css' | asset_url | stylesheet_tag }}
{{ 'component-list-social.css' | asset_url | stylesheet_tag }}

{%- style -%}
  .footer-v2 {
    margin-top: {{ section.settings.margin_top | times: 0.75 | round: 0 }}px;
  }

  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .footer-v2 {
      margin-top: {{ section.settings.margin_top }}px;
    }

    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%}

<style>
  /* Mobile Viewport Fix - Prevent white space from browser UI */
  @media screen and (max-width: 749px) {
    html {
      /* Fallback for older browsers */
      height: 100vh;
      /* Use small viewport height that excludes browser UI */
      height: 100svh;
    }

    body {
      /* Prevent overflow that causes white space */
      overflow-x: hidden;
      position: relative;
      /* Ensure body doesn't exceed viewport */
      max-height: 100vh;
      max-height: 100svh;
    }
  }

  /* Footer V2 - Clean Modern Design */
  .footer-v2 {
    background: rgba(var(--color-background), 1);
    color: rgba(var(--color-foreground), 1);
    position: relative;
    overflow-x: hidden;
    contain: layout;
    transform: translateZ(0);
  }

  .footer-v2__container {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 2rem;
    width: 100%;
  }


  /* Main Content Grid */
  .footer-v2__content {
    display: grid;
    grid-template-columns: 2fr 1.2fr;
    gap: 2.5rem;
    padding: 2.5rem 0 3rem;
    align-items: start;
    position: relative;
  }

  /* Quick Links Section */
  .footer-v2__links {
    grid-column: 1;
  }

  .footer-v2__heading {
    font-size: 1.3rem;
    font-weight: 600;
    color: rgba(var(--color-foreground), 1);
    margin: 0 0 1.5rem;
    letter-spacing: 0.02em;
    line-height: 1.2;
  }

  .footer-v2__menu {
    list-style: none;
    padding: 0;
    margin: 0;
  }

  .footer-v2__menu-item {
    margin-bottom: 0.75rem;
  }

  .footer-v2__menu-link {
    color: rgba(var(--color-foreground), 0.8);
    text-decoration: none;
    font-size: 1rem;
    line-height: 1.6;
    transition: all 0.3s ease;
    display: block;
    padding: 0.25rem 0;
  }

  .footer-v2__menu-link:hover {
    color: rgba(var(--color-foreground), 1);
    transform: translateX(4px);
  }

  /* Newsletter Section */
  .footer-v2__newsletter {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    top: 0;
    text-align: center;
    width: 350px;
    z-index: 1;
  }

  .footer-v2__newsletter-brand {
    font-size: 2.2rem;
    font-weight: 700;
    color: rgba(var(--color-foreground), 1);
    margin: -0.5rem 0 0.5rem;
    letter-spacing: 0.1em;
    text-transform: uppercase;
  }

  .footer-v2__newsletter-subtitle {
    font-size: 0.95rem;
    color: rgba(var(--color-foreground), 0.7);
    margin: 0 0 1.25rem;
    line-height: 1.4;
  }

  .footer-v2__newsletter-form {
    display: flex;
    flex-direction: column;
    gap: 1rem;
    width: 100%;
    max-width: 350px;
    margin: 0 auto;
  }

  .footer-v2__email-input {
    height: 48px;
    padding: 0 1rem;
    border: 1px solid rgba(var(--color-foreground), 0.2);
    border-radius: 0;
    font-size: 1rem;
    background: rgba(var(--color-background), 1);
    color: rgba(var(--color-foreground), 1);
    transition: border-color 0.3s ease;
  }

  .footer-v2__email-input:focus {
    border-color: rgba(var(--color-foreground), 0.5);
    outline: none;
  }

  .footer-v2__submit-btn {
    height: 48px;
    background: rgba(var(--color-foreground), 1);
    color: rgba(var(--color-background), 1);
    border: none;
    font-size: 1rem;
    font-weight: 600;
    letter-spacing: 0.05em;
    cursor: pointer;
    transition: all 0.3s ease;
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 0.5rem;
  }

  .footer-v2__submit-btn:hover {
    background: rgba(var(--color-foreground), 0.8);
    transform: translateY(-1px);
  }

  .footer-v2__form-message {
    font-size: 0.875rem;
    margin-top: 0.5rem;
  }

  .footer-v2__form-message--success {
    color: #22c55e;
  }

  .footer-v2__form-message--error {
    color: #ef4444;
  }

  /* Social Section */
  .footer-v2__social-only {
    grid-column: 2;
    display: flex;
    flex-direction: column;
    justify-self: end;
    text-align: left;
  }

  .footer-v2__social-links {
    display: flex;
    flex-direction: column;
    gap: 1rem;
    margin: 0;
    padding: 0;
    list-style: none;
    align-items: flex-start;
  }

  .footer-v2__social-item {
    list-style: none;
  }

  .footer-v2__social-link {
    display: flex;
    align-items: center;
    gap: 0.75rem;
    color: rgba(var(--color-foreground), 0.8);
    text-decoration: none;
    transition: all 0.3s ease;
    padding: 0.5rem 0;
    font-size: 1rem;
    line-height: 1;
  }

  .footer-v2__social-link:hover {
    color: rgba(var(--color-foreground), 1);
    transform: translateX(4px);
  }

  .footer-v2__social-icon {
    width: 24px;
    height: 24px;
    fill: currentColor;
    flex-shrink: 0;
    vertical-align: middle;
    display: block;
  }

  .footer-v2__payment-bottom {
    display: flex;
    gap: 0.75rem;
    justify-content: center;
    flex-wrap: wrap;
    align-items: center;
    margin-bottom: 1.5rem;
  }

  .footer-v2__payment-icon {
    width: 36px;
    height: 24px;
    opacity: 0.8;
    transition: opacity 0.3s ease;
  }

  .footer-v2__payment-icon:hover {
    opacity: 1;
  }

  /* Footer Bottom */
  .footer-v2__bottom {
    border-top: 1px solid rgba(var(--color-foreground), 0.08);
    padding: 2rem 0;
    text-align: center;
  }

  .footer-v2__policies {
    display: flex;
    justify-content: center;
    gap: 2rem;
    margin-bottom: 1rem;
    flex-wrap: wrap;
  }

  .footer-v2__policy-link {
    color: rgba(var(--color-foreground), 0.6);
    text-decoration: none;
    font-size: 0.875rem;
    transition: color 0.3s ease;
  }

  .footer-v2__policy-link:hover {
    color: rgba(var(--color-foreground), 1);
  }

  .footer-v2__copyright {
    color: rgba(var(--color-foreground), 0.5);
    font-size: 0.875rem;
    margin: 0;
  }

  /* Responsive Design */
  @media screen and (max-width: 989px) {
    .footer-v2__content {
      grid-template-columns: 1fr 1fr;
      gap: 3rem;
    }

    .footer-v2__links {
      grid-column: 1;
    }

    .footer-v2__newsletter {
      position: static;
      transform: none;
      left: auto;
      top: auto;
      width: auto;
      grid-column: 1 / 3;
      text-align: center;
      margin-top: 2rem;
    }

    .footer-v2__social-only {
      grid-column: 1 / 3;
      text-align: center;
      margin-top: 2rem;
    }

    .footer-v2__social-links {
      align-items: center;
    }
  }

  @media screen and (max-width: 749px) {
    .footer-v2 {
      /* Fix mobile browser UI viewport issues */
      -webkit-overflow-scrolling: touch;
      touch-action: manipulation;
      /* Add safe area for device bottom indicators */
      padding-bottom: calc(1rem + env(safe-area-inset-bottom, 0));
      /* Prevent layout shifts during browser UI changes */
      contain: layout style;
      transform: translate3d(0, 0, 0);
    }

    .footer-v2__container {
      padding: 0 1.5rem;
      width: 100%;
      box-sizing: border-box;
      /* Ensure stable layout */
      position: relative;
      isolation: isolate;
    }

    .footer-v2__content {
      grid-template-columns: 1fr;
      gap: 0;
      padding: 1.5rem 0 2rem;
    }

    .footer-v2__links,
    .footer-v2__social-only {
      display: none;
    }

    .footer-v2__newsletter {
      position: static;
      transform: none;
      left: auto;
      top: auto;
      width: auto;
      grid-column: 1;
      text-align: center;
    }

    .footer-v2__heading {
      font-size: 1.2rem;
      margin-bottom: 1rem;
    }

    .footer-v2__menu-link {
      font-size: 0.95rem;
    }

    .footer-v2__policies {
      flex-direction: column;
      gap: 1rem;
    }

    .footer-v2__social-links {
      align-items: center;
      gap: 1rem;
    }

  }

  @media screen and (max-width: 480px) {
    .footer-v2__container {
      padding: 0 1rem;
    }

    .footer-v2__brand {
      padding: 2rem 0 1.5rem;
    }

    .footer-v2__brand-title {
      font-size: 1.8rem;
    }

    .footer-v2__content {
      gap: 1.5rem;
      padding: 1.5rem 0;
    }

    .footer-v2__bottom {
      padding: 1.5rem 0 2rem;
    }
  }
</style>

<footer class="footer-v2 color-{{ section.settings.color_scheme }} gradient section-{{ section.id }}-padding">
  {%- liquid
    assign has_social_icons = false
    if settings.social_facebook_link != blank or settings.social_instagram_link != blank or settings.social_youtube_link != blank or settings.social_tiktok_link != blank or settings.social_twitter_link != blank or settings.social_pinterest_link != blank or settings.social_snapchat_link != blank or settings.social_tumblr_link != blank or settings.social_vimeo_link != blank
      assign has_social_icons = true
    endif
  -%}

  <div class="footer-v2__container">

    <!-- Main Content -->
    <div class="footer-v2__content">
      <!-- Quick Links -->
      <div class="footer-v2__links">
        <h3 class="footer-v2__heading">{{ section.settings.links_heading | default: 'Quick Links' }}</h3>
        {%- if section.settings.footer_menu != blank -%}
          <ul class="footer-v2__menu">
            {%- for link in section.settings.footer_menu.links -%}
              <li class="footer-v2__menu-item">
                <a href="{{ link.url }}" class="footer-v2__menu-link">{{ link.title }}</a>
              </li>
            {%- endfor -%}
          </ul>
        {%- else -%}
          <ul class="footer-v2__menu">
            <li class="footer-v2__menu-item">
              <a href="/pages/tech-pack-submission" class="footer-v2__menu-link">Access our Garment Studio to upload your technical files</a>
            </li>
            <li class="footer-v2__menu-item">
              <a href="/pages/futuristic" class="footer-v2__menu-link">Check our factory & showroom</a>
            </li>
            <li class="footer-v2__menu-item">
              <a href="/pages/how-does-it-work" class="footer-v2__menu-link">Get Started & FAQ's</a>
            </li>
            <li class="footer-v2__menu-item">
              <a href="/pages/design-services" class="footer-v2__menu-link">Design Services - make your ideas a reality with us</a>
            </li>
          </ul>
        {%- endif -%}
      </div>

      <!-- Newsletter -->
      {%- if section.settings.newsletter_enable -%}
        <div class="footer-v2__newsletter">
          <h1 class="footer-v2__newsletter-brand">{{ section.settings.brand_title | default: 'GENUINEBLANKS' }}</h1>
          <p class="footer-v2__newsletter-subtitle">{{ section.settings.brand_subtitle | default: 'Exclusive deals & insider updates for fashion brands' }}</p>
          {%- form 'customer', id: 'ContactFooter', class: 'footer-v2__newsletter-form' -%}
            <input type="hidden" name="contact[tags]" value="newsletter">
            <input
              id="NewsletterForm--{{ section.id }}"
              type="email"
              name="contact[email]"
              class="footer-v2__email-input"
              value="{{ form.email }}"
              aria-required="true"
              autocorrect="off"
              autocapitalize="off"
              autocomplete="email"
              placeholder="{{ section.settings.newsletter_placeholder | default: 'Enter your email address' }}"
              required
            >
            <button
              type="submit"
              class="footer-v2__submit-btn"
              name="commit"
              id="Subscribe"
              aria-label="{{ section.settings.newsletter_button | default: 'Subscribe' }}"
            >
              {{ section.settings.newsletter_button | default: 'SIGN UP' }}
            </button>
            {%- if form.errors -%}
              <div class="footer-v2__form-message footer-v2__form-message--error">
                {{- form.errors.translated_fields.email | capitalize }}
                {{ form.errors.messages.email -}}
              </div>
            {%- endif -%}
            {%- if form.posted_successfully? -%}
              <div class="footer-v2__form-message footer-v2__form-message--success">
                {{- 'newsletter.success' | t }}
              </div>
            {%- endif -%}
          {%- endform -%}
        </div>
      {%- endif -%}

      <!-- Social -->
      {%- if section.settings.show_social and has_social_icons -%}
        <div class="footer-v2__social-only">
          <h3 class="footer-v2__heading">Follow Us</h3>
          <ul class="footer-v2__social-links">
            {%- if settings.social_instagram_link != blank -%}
              <li class="footer-v2__social-item">
                <a href="{{ settings.social_instagram_link }}" class="footer-v2__social-link" target="_blank" rel="noopener" aria-label="Instagram">
                  <svg class="footer-v2__social-icon" aria-hidden="true" focusable="false" viewBox="0 0 24 24">
                    <path d="M12 2.163c3.204 0 3.584.012 4.85.07 3.252.148 4.771 1.691 4.919 4.919.058 1.265.069 1.645.069 4.849 0 3.205-.012 3.584-.069 4.849-.149 3.225-1.664 4.771-4.919 4.919-1.266.058-1.644.07-4.85.07-3.204 0-3.584-.012-4.849-.07-3.26-.149-4.771-1.699-4.919-4.92-.058-1.265-.07-1.644-.07-4.849 0-3.204.013-3.583.07-4.849.149-3.227 1.664-4.771 4.919-4.919 1.266-.057 1.645-.069 4.849-.069zm0-2.163c-3.259 0-3.667.014-4.947.072-4.358.2-6.78 2.618-6.98 6.98-.059 1.281-.073 1.689-.073 4.948 0 3.259.014 3.668.072 4.948.2 4.358 2.618 6.78 6.98 6.98 1.281.058 1.689.072 4.948.072 3.259 0 3.668-.014 4.948-.072 4.354-.2 6.782-2.618 6.979-6.98.059-1.28.073-1.689.073-4.948 0-3.259-.014-3.667-.072-4.947-.196-4.354-2.617-6.78-6.979-6.98-1.281-.059-1.69-.073-4.949-.073zm0 5.838c-3.403 0-6.162 2.759-6.162 6.162s2.759 6.163 6.162 6.163 6.162-2.759 6.162-6.163c0-3.403-2.759-6.162-6.162-6.162zm0 10.162c-2.209 0-4-1.79-4-4 0-2.209 1.791-4 4-4s4 1.791 4 4c0 2.21-1.791 4-4 4zm6.406-11.845c-.796 0-1.441.645-1.441 1.44s.645 1.44 1.441 1.44c.795 0 1.439-.645 1.439-1.44s-.644-1.44-1.439-1.44z"/>
                  </svg>
                  Instagram
                </a>
              </li>
            {%- endif -%}
            {%- if settings.social_facebook_link != blank -%}
              <li class="footer-v2__social-item">
                <a href="{{ settings.social_facebook_link }}" class="footer-v2__social-link" target="_blank" rel="noopener" aria-label="Facebook">
                  <svg class="footer-v2__social-icon" aria-hidden="true" focusable="false" viewBox="0 0 24 24">
                    <path d="M24 12.073c0-6.627-5.373-12-12-12s-12 5.373-12 12c0 5.99 4.388 10.954 10.125 11.854v-8.385H7.078v-3.47h3.047V9.43c0-3.007 1.792-4.669 4.533-4.669 1.312 0 2.686.235 2.686.235v2.953H15.83c-1.491 0-1.956.925-1.956 1.874v2.25h3.328l-.532 3.47h-2.796v8.385C19.612 23.027 24 18.062 24 12.073z"/>
                  </svg>
                  Facebook
                </a>
              </li>
            {%- endif -%}
            {%- if settings.social_twitter_link != blank -%}
              <li class="footer-v2__social-item">
                <a href="{{ settings.social_twitter_link }}" class="footer-v2__social-link" target="_blank" rel="noopener" aria-label="Twitter">
                  <svg class="footer-v2__social-icon" aria-hidden="true" focusable="false" viewBox="0 0 24 24">
                    <path d="M23.953 4.57a10 10 0 01-2.825.775 4.958 4.958 0 002.163-2.723c-.951.555-2.005.959-3.127 1.184a4.92 4.92 0 00-8.384 4.482C7.69 8.095 4.067 6.13 1.64 3.162a4.822 4.822 0 00-.666 2.475c0 1.71.87 3.213 2.188 4.096a4.904 4.904 0 01-2.228-.616v.06a4.923 4.923 0 003.946 4.827 4.996 4.996 0 01-2.212.085 4.936 4.936 0 004.604 3.417 9.867 9.867 0 01-6.102 2.105c-.39 0-.779-.023-1.17-.067a13.995 13.995 0 007.557 2.209c9.053 0 13.998-7.496 13.998-13.985 0-.21 0-.42-.015-.63A9.935 9.935 0 0024 4.59z"/>
                  </svg>
                  Twitter
                </a>
              </li>
            {%- endif -%}
            {%- if settings.social_tiktok_link != blank -%}
              <li class="footer-v2__social-item">
                <a href="{{ settings.social_tiktok_link }}" class="footer-v2__social-link" target="_blank" rel="noopener" aria-label="TikTok">
                  <svg class="footer-v2__social-icon" aria-hidden="true" focusable="false" viewBox="0 0 24 24">
                    <path d="M12.525.02c1.31-.02 2.61-.01 3.91-.02.08 1.53.63 3.09 1.75 4.17 1.12 1.11 2.7 1.62 4.24 1.79v4.03c-1.44-.05-2.89-.35-4.2-.97-.57-.26-1.1-.59-1.62-.93-.01 2.92.01 5.84-.02 8.75-.08 1.4-.54 2.79-1.35 3.94-1.31 1.92-3.58 3.17-5.91 3.21-1.43.08-2.86-.31-4.08-1.03-2.02-1.19-3.44-3.37-3.65-5.71-.02-.5-.03-1-.01-1.49.18-1.9 1.12-3.72 2.58-4.96 1.66-1.44 3.98-2.13 6.15-1.72.02 1.48-.04 2.96-.04 4.44-.99-.32-2.15-.23-3.02.37-.63.41-1.11 1.04-1.36 1.75-.21.51-.15 1.07-.14 1.61.24 1.64 1.82 3.02 3.5 2.87 1.12-.01 2.19-.66 2.77-1.61.19-.33.4-.67.41-1.06.1-1.79.06-3.57.07-5.36.01-4.03-.01-8.05.02-12.07z"/>
                  </svg>
                  TikTok
                </a>
              </li>
            {%- endif -%}
            {%- if settings.social_youtube_link != blank -%}
              <li class="footer-v2__social-item">
                <a href="{{ settings.social_youtube_link }}" class="footer-v2__social-link" target="_blank" rel="noopener" aria-label="YouTube">
                  <svg class="footer-v2__social-icon" aria-hidden="true" focusable="false" viewBox="0 0 24 24">
                    <path d="M23.498 6.186a3.016 3.016 0 0 0-2.122-2.136C19.505 3.545 12 3.545 12 3.545s-7.505 0-9.377.505A3.017 3.017 0 0 0 .502 6.186C0 8.07 0 12 0 12s0 3.93.502 5.814a3.016 3.016 0 0 0 2.122 2.136c1.871.505 9.376.505 9.376.505s7.505 0 9.377-.505a3.015 3.015 0 0 0 2.122-2.136C24 15.93 24 12 24 12s0-3.93-.502-5.814zM9.545 15.568V8.432L15.818 12l-6.273 3.568z"/>
                  </svg>
                  YouTube
                </a>
              </li>
            {%- endif -%}
          </ul>
        </div>
      {%- endif -%}
    </div>

    <!-- Footer Bottom -->
    <div class="footer-v2__bottom">
      <!-- Payment Icons -->
      {%- if section.settings.payment_enable -%}
        <div class="footer-v2__payment-bottom">
          {%- assign enabled_payment_types = 'apple_pay,master,paypal,visa' | remove: ' ' | split: ',' -%}
          {%- for type in enabled_payment_types -%}
            {{ type | payment_type_svg_tag: class: 'footer-v2__payment-icon' }}
          {%- endfor -%}
        </div>
      {%- endif -%}

      <!-- Policies -->
      {%- if section.settings.show_policy or section.settings.policy_menu != blank -%}
        <div class="footer-v2__policies">
          {%- if section.settings.policy_menu != blank -%}
            {%- for link in section.settings.policy_menu.links -%}
              <a href="{{ link.url }}" class="footer-v2__policy-link">{{ link.title }}</a>
            {%- endfor -%}
          {%- endif -%}
          {%- if section.settings.show_policy -%}
            {%- for policy in shop.policies -%}
              {%- if policy != blank -%}
                <a href="{{ policy.url }}" class="footer-v2__policy-link">{{ policy.title }}</a>
              {%- endif -%}
            {%- endfor -%}
          {%- endif -%}
        </div>
      {%- endif -%}

      <!-- Copyright -->
      <p class="footer-v2__copyright">
        {{ section.settings.copyright_text | default: '© 2025, Genuine Blanks' }}
      </p>
    </div>
  </div>
</footer>

{% schema %}
{
  "name": "Footer V2",
  "settings": [
    {
      "type": "color_scheme",
      "id": "color_scheme",
      "label": "Color scheme",
      "default": "scheme-1"
    },
    {
      "type": "header",
      "content": "Brand Section"
    },
    {
      "type": "text",
      "id": "brand_title",
      "label": "Brand title",
      "default": "GENUINEBLANKS"
    },
    {
      "type": "text",
      "id": "brand_subtitle",
      "label": "Brand subtitle",
      "default": "Exclusive deals & insider updates for fashion brands"
    },
    {
      "type": "header",
      "content": "Quick Links"
    },
    {
      "type": "text",
      "id": "links_heading",
