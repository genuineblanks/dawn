    <script>
    document.addEventListener('DOMContentLoaded', function() {
      
      // ===============================================
      // MAKE PRODUCT CARDS CLICKABLE
      // ===============================================
      
      function makeCardsClickable() {
        console.log('🎯 Making product cards clickable...');
        
        const productCards = document.querySelectorAll(
          '.product-card-wrapper, .card-wrapper, .product-recommendations .card-wrapper, .you-may-also-like .card-wrapper, .card, .collection .card-wrapper'
        );
        
        productCards.forEach(card => {
          // Skip if already processed
          if (card.hasAttribute('data-clickable-processed')) return;
          
          // Find the main product link
          const productLink = card.querySelector(
            '.card__heading a, .card__information a, .card__media a, a[href*="/products/"]'
          );
          
          if (productLink && productLink.href) {
            const productUrl = productLink.href;
            
            // Mark as processed
            card.setAttribute('data-clickable-processed', 'true');
            card.style.cursor = 'pointer';
            
            // Add click handler
            card.addEventListener('click', function(e) {
              // Don't trigger if clicking on interactive elements
              if (e.target.closest('button, .btn, input, select, .swatch, .quantity, .product-form__buttons, [role="button"], .shopify-payment-button')) {
                return;
              }
              
              // Don't trigger if clicking on another link
              if (e.target.closest('a') && e.target.closest('a') !== productLink) {
                return;
              }
              
              // Navigate to product page
              if (e.ctrlKey || e.metaKey) {
                // Open in new tab if Ctrl/Cmd+click
                window.open(productUrl, '_blank');
              } else {
                window.location.href = productUrl;
              }
            });
            
            // Add keyboard support
            card.setAttribute('tabindex', '0');
            card.setAttribute('role', 'link');
            card.setAttribute('aria-label', 'View product: ' + (productLink.textContent || 'Product'));
            
            card.addEventListener('keydown', function(e) {
              if (e.key === 'Enter' || e.key === ' ') {
                e.preventDefault();
                window.location.href = productUrl;
              }
            });
            
            console.log('✅ Made card clickable:', productUrl);
          } else {
            console.log('❌ No product link found in card:', card);
          }
        });
        
        console.log(`🎯 Processed ${productCards.length} product cards`);
      }
      
      // ===============================================
      // ENHANCED DAWN GALLERY FUNCTIONALITY
      // ===============================================
      
      function enhanceDawnGallery() {
        console.log('🖼️ Enhancing Dawn gallery...');
        
        // Find the main product image
        const mainImage = document.querySelector('.product__media img, .product-media img, .media img, img[src*="product"]');
        
        // Find thumbnail containers
        const thumbnails = document.querySelectorAll('.product__media-toggle, .thumbnail-list button, .slider-counter__link, [data-media-id], .thumbnail.global-media-settings');
        
        console.log(`🎯 Main image found:`, !!mainImage);
        console.log(`🎯 Thumbnails found:`, thumbnails.length);
        
        if (mainImage && thumbnails.length > 0) {
          console.log('✅ Setting up enhanced gallery functionality...');
          
          // Add click handlers to thumbnails
          thumbnails.forEach((thumbnail, index) => {
            // Skip if already processed
            if (thumbnail.hasAttribute('data-gallery-processed')) return;
            thumbnail.setAttribute('data-gallery-processed', 'true');
            
            thumbnail.addEventListener('click', function(e) {
              e.preventDefault();
              
              // Find the image source
              const img = this.querySelector('img');
              const newSrc = img?.src || this.href || this.dataset.src;
              
              if (newSrc && mainImage) {
                console.log(`🔄 Switching to image: ${newSrc}`);
                
                // Smooth transition
                mainImage.style.opacity = '0.7';
                mainImage.style.transform = 'scale(0.98)';
                
                setTimeout(() => {
                  mainImage.src = newSrc;
                  mainImage.style.opacity = '1';
                  mainImage.style.transform = 'scale(1)';
                }, 200);
                
                // Update active states
                thumbnails.forEach(t => {
                  t.classList.remove('active');
                  t.removeAttribute('aria-current');
                });
                this.classList.add('active');
                this.setAttribute('aria-current', 'true');
              }
            });
            
            // Add keyboard support
            if (!thumbnail.hasAttribute('tabindex')) {
              thumbnail.setAttribute('tabindex', '0');
            }
            thumbnail.addEventListener('keydown', function(e) {
              if (e.key === 'Enter' || e.key === ' ') {
                e.preventDefault();
                this.click();
              }
            });
          });
          
          // Add zoom functionality to main image
          if (mainImage && !mainImage.hasAttribute('data-zoom-processed')) {
            mainImage.setAttribute('data-zoom-processed', 'true');
            let isZoomed = false;
            
            mainImage.addEventListener('click', function() {
              isZoomed = !isZoomed;
              const container = this.closest('.product__media, .product-media, .media');
              
              if (container) {
                container.style.overflow = isZoomed ? 'hidden' : 'visible';
                this.style.transform = isZoomed ? 'scale(2)' : 'scale(1)';
                this.style.cursor = isZoomed ? 'zoom-out' : 'zoom-in';
                this.style.transition = 'transform 0.3s ease';
              }
            });
            
            mainImage.addEventListener('mousemove', function(e) {
              if (isZoomed) {
                const rect = this.getBoundingClientRect();
                const x = ((e.clientX - rect.left) / rect.width) * 100;
                const y = ((e.clientY - rect.top) / rect.height) * 100;
                this.style.transformOrigin = `${x}% ${y}%`;
              }
            });
          }
          
          console.log('✅ Enhanced gallery functionality activated!');
        } else {
          console.log('❌ Could not find required gallery elements');
        }
      }
      
      // ===============================================
      // INITIALIZATION AND MONITORING
      // ===============================================
      
      // Run functions initially
      makeCardsClickable();
      setTimeout(enhanceDawnGallery, 500);
      
      // Monitor for dynamically loaded content
      const observer = new MutationObserver(function(mutations) {
        let shouldReprocess = false;
        
        mutations.forEach(function(mutation) {
          if (mutation.addedNodes.length > 0) {
            // Check if new product cards or images were added
            const hasNewContent = Array.from(mutation.addedNodes).some(node => 
              node.nodeType === Node.ELEMENT_NODE && 
              (node.matches('.product-card-wrapper, .card-wrapper, .card, img') || 
               node.querySelector('.product-card-wrapper, .card-wrapper, .card, img'))
            );
            
            if (hasNewContent) {
              shouldReprocess = true;
            }
          }
        });
        
        if (shouldReprocess) {
          console.log('🔄 New content detected, re-processing...');
          setTimeout(() => {
            makeCardsClickable();
            enhanceDawnGallery();
          }, 100);
        }
      });
      
      // Start observing
      observer.observe(document.body, {
        childList: true,
        subtree: true
      });
      
      // Handle Shopify section loading
      document.addEventListener('shopify:section:load', function() {
        console.log('🔄 Shopify section loaded, re-processing...');
        setTimeout(() => {
          makeCardsClickable();
          enhanceDawnGallery();
        }, 200);
      });
      
      // Handle AJAX loading for recommendations
      document.addEventListener('DOMContentLoaded', function() {
        // Look for recommendation sections that might load via AJAX
        const recommendationSections = document.querySelectorAll('.product-recommendations, .you-may-also-like, [data-recommendations]');
        
        recommendationSections.forEach(section => {
          // Set up observer for this specific section
          const sectionObserver = new MutationObserver(function(mutations) {
            mutations.forEach(function(mutation) {
              if (mutation.addedNodes.length > 0) {
                console.log('🔄 Recommendation section updated, re-processing...');
                setTimeout(makeCardsClickable, 100);
              }
            });
          });
          
          sectionObserver.observe(section, {
            childList: true,
            subtree: true
          });
        });
      });
      
      console.log('🚀 Product card enhancement complete!');
    });
    </script>

